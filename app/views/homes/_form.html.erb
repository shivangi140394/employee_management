<%#= form_for :home, :url => employee_homes_path  do |f| %>
<%= form_for(@home, :url => employee_homes_path, :validate => true, :html => { :'data-ajax' => false }) do |f| %>
  
   <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
 
  <div>
    <%= f.label :body %>
    <%= f.text_area :body %>
  </div>
 
  <div>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <p><%= f.submit %></p>
<% end %>